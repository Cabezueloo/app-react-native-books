{
  "expo": {
    "name": "app-react-native-books",
    "slug": "app-react-native-books",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "books",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2C3E50"
      },
      "package": "com.angel_18.appreactnativebooks"
    },
    "web": {
      "favicon": "./app/assets/favicon.png",
      "bundler": "metro"
    },
    "sdkVersion": "52.0.0",
    "plugins": [
      "expo-router"
    ]
  },
  "name": "app-react-native-books"
}
